{
    "name": "dbt/type-checker",
    "description": "A type checker for PHP",
    "keywords": [
        "dbt",
        "type-checker"
    ],
    "homepage": "https://github.com/dbt/type-checker",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Daniel S. Deboer",
            "email": "daniel.s.deboer@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "vimeo/psalm": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Dbt\\TypeChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dbt\\TypeChecker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --no-coverage",
        "test-coverage": "phpunit --coverage-html coverage",
        "stat": "psalm"
    },
    "config": {
        "sort-packages": true
    }
}
